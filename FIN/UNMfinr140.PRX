		Endif
		
		If mv_par03 == 1  // 1-Verifica Filial
			dbSetOrder(1)
			dbSeek(cFilial+DTOS(dDataBase),.T.)
		Else
			dbSetOrder(nIndexSE5)
			dbSeek(DTOS(dDataBase),.T.)
		Endif
		
		While &cCondSE5
			If mv_par14 == 1
